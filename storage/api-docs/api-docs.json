        "/api/v1/users/{userId}/todos": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get a list of todos for a specific user",
                "operationId": "userGetTodos",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "UUID of the user to retrieve todos for",
                        "required": true,
                        "schema": {
                            "type": "string",
